{
  "name": "togglelayers",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "toggleLayers",
    "manifest": "src/manifest.json",
    "main": "togglelayers.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinGutowski/toggleLayers"
  },
  "author": "Kevin Gutowski <kgutowski@berkeley.edu>",
  "description": "One action to toggle layers on / off so that you can more easily remap this to a custom keyboard shortcut."
}
